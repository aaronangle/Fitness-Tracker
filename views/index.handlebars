<div class="container-fluid wrapper">
    <br>
    <br>
    <br>
    <div class="row center">
        <div class="col-4 col-lg-3 sidenav">
            {{#if data}}
            <h3 class="text-center">Your Workouts</h3>
            <hr>
            {{#each data}}
            <button type="button" class="btn btn-outline-primary btn-lg btn-block viewWorkout"
                data-id={{_id}}>{{workout}}</button>
            {{/each}}
            {{/if}}
        </div>
        <div class="col-8 col-lg-6">
            <div class="card text-center">
                <div class="card-header">

                </div>
                <div class="card-body">
                    <h5 class="card-title">Track your Workouts</h5>
                    <p class="card-text">Track the workouts you do by adding a new workout</p>
                    <a href="#" id="addWorkout" class="btn btn-primary">Add Workout</a>
                </div>
                <div class="card-footer text-muted">

                </div>
            </div>
        </div>
        <div class="col-0 col-lg-3"></div>
    </div>
</div>